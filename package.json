    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.2",
